Cell[BoxData[
 RowBox[{
  RowBox[{"solveIt", "[", "x_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"constraints", ",", "equations", ",", "elim"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"constraints", " ", "=", " ", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Numerator", "@", 
          RowBox[{"Together", "@", "#"}]}], " ", "&"}], "/@", 
        RowBox[{"(", 
         RowBox[{"Expand", "@", 
          RowBox[{"stringDiff", "@", "x"}]}], ")"}]}], " ", "/.", 
       "mrules"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"equations", " ", "=", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"#", "\[Equal]", "0"}], "&"}], "/@", 
       RowBox[{"Last", "/@", 
        RowBox[{"ArrayRules", "@", 
         RowBox[{"Last", "@", 
          RowBox[{"CoefficientArrays", "[", 
           RowBox[{"constraints", ",", "minvts"}], "]"}]}]}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"elim", " ", "=", " ", 
      RowBox[{"constraints", "~", "extractType", "~", "b"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ToRules", "@", 
      RowBox[{"Reduce", "[", 
       RowBox[{"equations", ",", "elim"}], "]"}]}]}]}], "]"}]}]], "Input",
