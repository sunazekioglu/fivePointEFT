Cell[BoxData[
 RowBox[{
  RowBox[{"wardQuickQ", "[", "expr_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"term", ",", "ward", ",", "numRules", ",", "numEval"}], "}"}], 
    ",", 
    RowBox[{
     RowBox[{"term", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Head", "[", "expr", "]"}], "===", "Function"}], ",", 
        RowBox[{"expr", "[", 
         RowBox[{"k", "/@", 
          RowBox[{"Range", "[", "5", "]"}]}], "]"}], ",", "expr"}], "]"}]}], 
     ";", 
     RowBox[{"ward", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"returnToBasis", "[", 
         RowBox[{"term", "/.", "\[VeryThinSpace]", 
          RowBox[{
           RowBox[{"\[CurlyEpsilon]", "[", 
            RowBox[{"k", "[", "#", "]"}], "]"}], "\[Rule]", 
           RowBox[{"k", "[", "#", "]"}]}]}], "]"}], "&"}], "@", 
       RowBox[{"RandomChoice", "@", 
        RowBox[{"Range", "[", "5", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"numRules", " ", "=", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"#", "\[Rule]", " ", 
         RowBox[{"RandomPrime", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"10", "^", "3"}], ",", 
            RowBox[{"10", "^", "6"}]}], "}"}], "]"}]}], "&"}], "/@", 
       RowBox[{"Join", "[", 
        RowBox[{"structureBasis", ",", "invts5"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"numEval", " ", "=", " ", 
      RowBox[{"Block", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
         "d5", ",", "d3f3f3", ",", "d4f3", ",", "f3d3f3", ",", "f3f3f3", ",", 
          "tr", ",", "ulp"}], "}"}], ",", 
        RowBox[{
         RowBox[{"numRules", "/.", 
          RowBox[{"Rule", "\[RuleDelayed]", " ", "Set"}]}], ";", "ward"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Together", "@", "numEval"}], "===", "0"}]}]}], 
   "]"}]}]], "Input",
